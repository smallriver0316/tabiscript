{
  "name": "tabiscript",
  "version": "0.0.1",
  "description": "Tabi Script is a comprehensive travel recording application that supports users throughout their entire travel journey - from pre-travel planning to post-travel blog creation.",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^24.7.0",
    "eslint": "^9.37.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "type-check": "npm run type-check --workspaces"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smallriver0316/tabiscript.git"
  },
  "keywords": [
    "Next.js",
    "ReactNative",
    "Supabase",
    "Mapbox"
  ],
  "author": "smallriver0316",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/smallriver0316/tabiscript/issues"
  },
  "homepage": "https://github.com/smallriver0316/tabiscript#readme"
